

Create Database MyDb;
use MyDb;

-- Create Students table
CREATE TABLE Students (
    id INT PRIMARY KEY IDENTITY(1,1),
    studentname VARCHAR(50) NOT NULL,
    Age INT,
    class VARCHAR(10),
    Rollno INT
);

-- Insert five records
INSERT INTO Students (studentname, Age, class, Rollno)
VALUES ('Arjun Sharma', 16, '10A', 101);

